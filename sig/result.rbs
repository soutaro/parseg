module Parseg
  class Result
    attr_reader token_locator: TokenLocator

    attr_reader tree: Tree::t

    attr_reader skip_tokens: Array[Integer]

    def initialize: (tree: Tree::t, token_locator: TokenLocator, skip_tokens: Array[Integer]) -> void

    def each_error_tree: (?Tree::t) { (Tree::MissingTree) -> void } -> void
                       | () -> Enumerator[Tree::MissingTree, void]
  end
end
